<%= form_for product do |f| -%>
	<div class="form-buttons well well-sm">
		<button class="btn btn-standard btn-success">Save</button>
		<button type="button" class="btn btn-standard btn-default" onclick="history.go(-1)">
			<i class="fa fa-mail-reply visible-sm visible-xs"></i><span class="visible-md visible-lg">Cancel</span>
		</button>
	</div>
<%= render :partial => 'entry', :locals => {:f => f} -%>
<%- end -%>
